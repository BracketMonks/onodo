.container-fluid
  %h3= t '.title'
  %table.table.table-striped
    %thead
      %tr
        %th #
        %th= t 'common.visualization'
        %th= t 'common.nodes'
        %th= t 'common.relations'
    %tbody
      - @datasets.each do |dataset|
        %tr
          %td= dataset.id
          - unless dataset.visualization.nil?
            %td= dataset.visualization.name
          - else
            %td
          - unless dataset.nodes.nil?
            %td= dataset.nodes.count
          - else
            %td
          - unless dataset.relations.nil?
            %td= dataset.relations.count
          - else
            %td
